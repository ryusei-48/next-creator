{
  "compilerOptions": {
    "baseUrl": "./",
    "typeRoots": ["src/custom-plugin/types"],
    //"types": [],
    "lib": [
      "ES2019",
      "ES2020.String",
      "DOM",
      "DOM.Iterable"
    ],
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  //"include": ["src/**/.*d.ts"]
}
